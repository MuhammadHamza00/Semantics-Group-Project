@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://www.fypont.org/fyp-process>
  a owl:Ontology ;
  rdfs:label "FypProOnt" ;
  rdfs:comment "Ontology to model the Final Year Project process including people, documents, evaluations, and organizational structure." .

<http://www.fypont.org/fyp-process#Student>
  a owl:Class ;
  rdfs:label "Student" ;
  rdfs:comment "A person undertaking a final year project." .

<http://www.fypont.org/fyp-process#Supervisor>
  a owl:Class ;
  rdfs:label "Supervisor" ;
  rdfs:comment "Faculty member responsible for guiding the student group." .

<http://www.fypont.org/fyp-process#Evaluator>
  a owl:Class ;
  rdfs:label "Evaluator" ;
  rdfs:comment "A person assigned to evaluate student projects." .

<http://www.fypont.org/fyp-process#Group>
  a owl:Class ;
  rdfs:label "Group" ;
  rdfs:comment "A team of students working on a single project." .

<http://www.fypont.org/fyp-process#Project>
  a owl:Class ;
  rdfs:label "Project" ;
  rdfs:comment "The final year project being undertaken." .

<http://www.fypont.org/fyp-process#ProjectType>
  a owl:Class ;
  rdfs:label "Project Type" ;
  rdfs:comment "The type/category of the project (e.g., research, development)." .

<http://www.fypont.org/fyp-process#Technology>
  a owl:Class ;
  rdfs:label "Technology" ;
  rdfs:comment "Tools or programming languages used in the project." .

<http://www.fypont.org/fyp-process#Proposal>
  a owl:Class ;
  rdfs:label "Proposal" ;
  rdfs:comment "Initial documentation outlining the project idea and plan." .

<http://www.fypont.org/fyp-process#FinalReport>
  a owl:Class ;
  rdfs:label "Final Report" ;
  rdfs:comment "Comprehensive documentation of the completed project." .

<http://www.fypont.org/fyp-process#Presentation>
  a owl:Class ;
  rdfs:label "Presentation" ;
  rdfs:comment "Slide deck or other material used to present the project." .

<http://www.fypont.org/fyp-process#Evaluation>
  a owl:Class ;
  rdfs:label "Evaluation" ;
  rdfs:comment "Assessment process or result for a project." .

<http://www.fypont.org/fyp-process#Department>
  a owl:Class ;
  rdfs:label "Department" ;
  rdfs:comment "Academic division within the university." .

<http://www.fypont.org/fyp-process#Committee>
  a owl:Class ;
  rdfs:label "Committee" ;
  rdfs:comment "Panel of evaluators or faculty overseeing project quality." .

<http://www.fypont.org/fyp-process#University>
  a owl:Class ;
  rdfs:label "University" ;
  rdfs:comment "Educational institution offering the program." .

<http://www.fypont.org/fyp-process#hasSupervisor>
  a owl:ObjectProperty ;
  rdfs:label "has Supervisor" ;
  rdfs:comment "Associates a group with its assigned supervisor." .

<http://www.fypont.org/fyp-process#hasEvaluator>
  a owl:ObjectProperty ;
  rdfs:label "has Evaluator" ;
  rdfs:comment "Links an evaluation to the evaluator." .

<http://www.fypont.org/fyp-process#hasGroup>
  a owl:ObjectProperty ;
  rdfs:label "has Group" ;
  rdfs:comment "Links a project to its associated student group." .

<http://www.fypont.org/fyp-process#hasProject>
  a owl:ObjectProperty ;
  rdfs:label "has Project" ;
  rdfs:comment "Assigns a project to a group or student." .

<http://www.fypont.org/fyp-process#authoredBy>
  a owl:ObjectProperty ;
  rdfs:label "authored By" ;
  rdfs:comment "Indicates the author of a document." .

<http://www.fypont.org/fyp-process#reviewedBy>
  a owl:ObjectProperty ;
  rdfs:label "reviewed By" ;
  rdfs:comment "Indicates the reviewer of a document or work." .

<http://www.fypont.org/fyp-process#usesTechnology>
  a owl:ObjectProperty ;
  rdfs:label "uses Technology" ;
  rdfs:comment "Indicates the technology used in a project." .

<http://www.fypont.org/fyp-process#hasDepartment>
  a owl:ObjectProperty ;
  rdfs:label "has Department" ;
  rdfs:comment "Indicates the departments under a university." .

<http://www.fypont.org/fyp-process#belongsToUniversity>
  a owl:ObjectProperty ;
  rdfs:label "belongs To University" ;
  rdfs:comment "Relates a department to its university." .

<http://www.fypont.org/fyp-process#memberOf>
  a owl:ObjectProperty ;
  rdfs:label "member Of" ;
  rdfs:comment "Indicates student membership in a group." .

<http://www.fypont.org/fyp-process#assignedTo>
  a owl:ObjectProperty ;
  rdfs:label "assigned To" ;
  rdfs:comment "Links a person or role to a task or project." .

<http://www.fypont.org/fyp-process#hasDocumentation>
  a owl:ObjectProperty ;
  rdfs:label "has Documentation" ;
  rdfs:comment "Links a project to its documentation artifacts." .

<http://www.fypont.org/fyp-process#studentID>
  a owl:DatatypeProperty ;
  rdfs:label "Student ID" ;
  rdfs:comment "Unique identifier for a student." .

<http://www.fypont.org/fyp-process#supervisorID>
  a owl:DatatypeProperty ;
  rdfs:label "Supervisor ID" ;
  rdfs:comment "Unique identifier for a supervisor." .

<http://www.fypont.org/fyp-process#email>
  a owl:DatatypeProperty ;
  rdfs:label "Email" ;
  rdfs:comment "Email address of a person." .

<http://www.fypont.org/fyp-process#projectTitle>
  a owl:DatatypeProperty ;
  rdfs:label "Project Title" ;
  rdfs:comment "Title of the project." .

<http://www.fypont.org/fyp-process#projectDescription>
  a owl:DatatypeProperty ;
  rdfs:label "Project Description" ;
  rdfs:comment "Brief summary of the project." .

<http://www.fypont.org/fyp-process#goal>
  a owl:DatatypeProperty ;
  rdfs:label "Goal" ;
  rdfs:comment "Objective or aim of the project." .

<http://www.fypont.org/fyp-process#startDate>
  a owl:DatatypeProperty ;
  rdfs:label "Start Date" ;
  rdfs:comment "Beginning date of the project." .

<http://www.fypont.org/fyp-process#endDate>
  a owl:DatatypeProperty ;
  rdfs:label "End Date" ;
  rdfs:comment "Completion date of the project." .

<http://www.fypont.org/fyp-process#createdDate>
  a owl:DatatypeProperty ;
  rdfs:label "Created Date" ;
  rdfs:comment "Timestamp of when a document or item was created." .

<http://www.fypont.org/fyp-process#grade>
  a owl:DatatypeProperty ;
  rdfs:label "Grade" ;
  rdfs:comment "Letter or qualitative grade for a project." .

<http://www.fypont.org/fyp-process#score>
  a owl:DatatypeProperty ;
  rdfs:label "Score" ;
  rdfs:comment "Numerical score in an evaluation." .

<http://www.fypont.org/fyp-process#evaluationDate>
  a owl:DatatypeProperty ;
  rdfs:label "Evaluation Date" ;
  rdfs:comment "Date on which evaluation was conducted." .

<http://www.fypont.org/fyp-process#technologyName>
  a owl:DatatypeProperty ;
  rdfs:label "Technology Name" ;
  rdfs:comment "Name of the technology used." .

<http://www.fypont.org/fyp-process#version>
  a owl:DatatypeProperty ;
  rdfs:label "Version" ;
  rdfs:comment "Version number technology." .

<http://www.fypont.org/fyp-process#category>
  a owl:DatatypeProperty ;
  rdfs:label "category" ;
  rdfs:comment "category of a project or document." .

<http://www.fypont.org/fyp-process#duration>
  a owl:DatatypeProperty ;
  rdfs:label "Duration" ;
  rdfs:comment "Time span or length of a project or presentation." .