<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
]>
<rdf:RDF xmlns="http://www.fypont.org/fyp-process#"
     xml:base="http://www.fypont.org/fyp-process"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:schema="http://schema.org/"
     xmlns:dcterms="http://purl.org/dc/terms/">

  <owl:Ontology rdf:about="http://www.fypont.org/fyp-process">
    <rdfs:label>FypProOnt</rdfs:label>
    <rdfs:comment>Ontology to model the Final Year Project process including people, documents, evaluations, and organizational structure.</rdfs:comment>
  </owl:Ontology>

  <!-- Reused Classes -->
  <owl:Class rdf:about="foaf:Person">
    <rdfs:label>Person</rdfs:label>
    <rdfs:comment>A general human individual, reused from FOAF.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="dcterms:Document">
    <rdfs:label>Document</rdfs:label>
    <rdfs:comment>A generic document entity from Dublin Core.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="schema:CollegeOrUniversity">
    <rdfs:label>College or University</rdfs:label>
    <rdfs:comment>Academic institution from Schema.org.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="schema:CreativeWork">
    <rdfs:label>Creative Work</rdfs:label>
    <rdfs:comment>Creative content such as proposals and reports from Schema.org.</rdfs:comment>
  </owl:Class>

  <!-- Classes -->
  <owl:Class rdf:about="#Student">
    <rdfs:label>Student</rdfs:label>
    <rdfs:comment>A person undertaking a final year project.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Supervisor">
    <rdfs:label>Supervisor</rdfs:label>
    <rdfs:comment>Faculty member responsible for guiding the student group.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Evaluator">
    <rdfs:label>Evaluator</rdfs:label>
    <rdfs:comment>A person assigned to evaluate student projects.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Group">
    <rdfs:label>Group</rdfs:label>
    <rdfs:comment>A team of students working on a single project.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Project">
    <rdfs:label>Project</rdfs:label>
    <rdfs:comment>The final year project being undertaken.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#ProjectType">
    <rdfs:label>Project Type</rdfs:label>
    <rdfs:comment>The type/category of the project (e.g., research, development).</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Technology">
    <rdfs:label>Technology</rdfs:label>
    <rdfs:comment>Tools or programming languages used in the project.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Proposal">
    <rdfs:label>Proposal</rdfs:label>
    <rdfs:comment>Initial documentation outlining the project idea and plan.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#FinalReport">
    <rdfs:label>Final Report</rdfs:label>
    <rdfs:comment>Comprehensive documentation of the completed project.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Presentation">
    <rdfs:label>Presentation</rdfs:label>
    <rdfs:comment>Slide deck or other material used to present the project.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Evaluation">
    <rdfs:label>Evaluation</rdfs:label>
    <rdfs:comment>Assessment process or result for a project.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Department">
    <rdfs:label>Department</rdfs:label>
    <rdfs:comment>Academic division within the university.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#Committee">
    <rdfs:label>Committee</rdfs:label>
    <rdfs:comment>Panel of evaluators or faculty overseeing project quality.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#University">
    <rdfs:label>University</rdfs:label>
    <rdfs:comment>Educational institution offering the program.</rdfs:comment>
  </owl:Class>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="#hasSupervisor">
    <rdfs:label>has Supervisor</rdfs:label>
    <rdfs:comment>Associates a group with its assigned supervisor.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasEvaluator">
    <rdfs:label>has Evaluator</rdfs:label>
    <rdfs:comment>Links an evaluation to the evaluator.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasGroup">
    <rdfs:label>has Group</rdfs:label>
    <rdfs:comment>Links a project to its associated student group.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasProject">
    <rdfs:label>has Project</rdfs:label>
    <rdfs:comment>Assigns a project to a group or student.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#authoredBy">
    <rdfs:label>authored By</rdfs:label>
    <rdfs:comment>Indicates the author of a document.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#reviewedBy">
    <rdfs:label>reviewed By</rdfs:label>
    <rdfs:comment>Indicates the reviewer of a document or work.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#usesTechnology">
    <rdfs:label>uses Technology</rdfs:label>
    <rdfs:comment>Indicates the technology used in a project.</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasDepartment">
    <rdfs:label>has Department</rdfs:label>
    <rdfs:comment>Indicates the departments under a university.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#belongsToUniversity">
    <rdfs:label>belongs To University</rdfs:label>
    <rdfs:comment>Relates a department to its university.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#memberOf">
    <rdfs:label>member Of</rdfs:label>
    <rdfs:comment>Indicates student membership in a group.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#assignedTo">
    <rdfs:label>assigned To</rdfs:label>
    <rdfs:comment>Links a person or role to a task or project.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasDocumentation">
    <rdfs:label>has Documentation</rdfs:label>
    <rdfs:comment>Links a project to its documentation artifacts.</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Data Properties -->
  <owl:DatatypeProperty rdf:about="#studentID">
    <rdfs:label>Student ID</rdfs:label>
    <rdfs:comment>Unique identifier for a student.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#supervisorID">
    <rdfs:label>Supervisor ID</rdfs:label>
    <rdfs:comment>Unique identifier for a supervisor.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#email">
    <rdfs:label>Email</rdfs:label>
    <rdfs:comment>Email address of a person.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#projectTitle">
    <rdfs:label>Project Title</rdfs:label>
    <rdfs:comment>Title of the project.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#projectDescription">
    <rdfs:label>Project Description</rdfs:label>
    <rdfs:comment>Brief summary of the project.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#goal">
    <rdfs:label>Goal</rdfs:label>
    <rdfs:comment>Objective or aim of the project.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#startDate">
    <rdfs:label>Start Date</rdfs:label>
    <rdfs:comment>Beginning date of the project.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#endDate">
    <rdfs:label>End Date</rdfs:label>
    <rdfs:comment>Completion date of the project.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#createdDate">
    <rdfs:label>Created Date</rdfs:label>
    <rdfs:comment>Timestamp of when a document or item was created.</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#grade">
    <rdfs:label>Grade</rdfs:label>
    <rdfs:comment>Letter or qualitative grade for a project.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#score">
    <rdfs:label>Score</rdfs:label>
    <rdfs:comment>Numerical score in an evaluation.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#evaluationDate">
    <rdfs:label>Evaluation Date</rdfs:label>
    <rdfs:comment>Date on which evaluation was conducted.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#technologyName">
    <rdfs:label>Technology Name</rdfs:label>
    <rdfs:comment>Name of the technology used.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#version">
    <rdfs:label>Version</rdfs:label>
    <rdfs:comment>Version number technology.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#category">
    <rdfs:label>category</rdfs:label>
    <rdfs:comment>category of a project or document.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#duration">
    <rdfs:label>Duration</rdfs:label>
    <rdfs:comment>Time span or length of a project or presentation.</rdfs:comment>
  </owl:DatatypeProperty>

</rdf:RDF>
